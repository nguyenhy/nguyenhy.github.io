{
  "name": "playground-core",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": "./dist/main.js",
    "./web": {
      "import": "./dist/web.js",
      "types": "./dist/web.d.ts"
    }
  },
  "module": "./dist/main.js",
